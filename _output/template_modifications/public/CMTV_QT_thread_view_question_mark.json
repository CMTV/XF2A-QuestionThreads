    "template": "thread_view",
    "description": "Adding thread marks before thread title",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": "<xf:h1>",
    "replace": "$0\n\n<xf:macro name=\"thread_marks\" template=\"CMTV_QT_thread_marks_macros\"\n\t\t  arg-context=\"thread_title\" arg-thread=\"{$thread}\" />"
